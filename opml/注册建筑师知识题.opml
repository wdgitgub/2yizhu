              <outline text="居住小区、" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%B1%85%E4%BD%8F%E5%B0%8F%E5%8C%BA%E3%80%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="组团" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E7%BB%84%E5%9B%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="居住区规模（包含人口规模和用地规模）" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%B1%85%E4%BD%8F%E5%8C%BA%E8%A7%84%E6%A8%A1%EF%BC%88%E5%8C%85%E5%90%AB%E4%BA%BA%E5%8F%A3%E8%A7%84%E6%A8%A1%E5%92%8C%E7%94%A8%E5%9C%B0%E8%A7%84%E6%A8%A1%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
