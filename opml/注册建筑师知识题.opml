          <outline text="居住区绿地蓄水、透水要求" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%B1%85%E4%BD%8F%E5%8C%BA%E7%BB%BF%E5%9C%B0%E8%93%84%E6%B0%B4%E3%80%81%E9%80%8F%E6%B0%B4%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
