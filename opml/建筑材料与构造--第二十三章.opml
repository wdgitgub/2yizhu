          <outline text="不宜选用：矿渣水泥、 火山灰水泥、粉煤灰水泥、复合水泥" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%AE%9C%E9%80%89%E7%94%A8%EF%BC%9A%E7%9F%BF%E6%B8%A3%E6%B0%B4%E6%B3%A5%E3%80%81%20%E7%81%AB%E5%B1%B1%E7%81%B0%E6%B0%B4%E6%B3%A5%E3%80%81%E7%B2%89%E7%85%A4%E7%81%B0%E6%B0%B4%E6%B3%A5%E3%80%81%E5%A4%8D%E5%90%88%E6%B0%B4%E6%B3%A5%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="2	严寒地区的露天混凝土，寒冷地区处于水位升降范围内的混凝土" _mubu_text="%3Cspan%3E2%09%E4%B8%A5%E5%AF%92%E5%9C%B0%E5%8C%BA%E7%9A%84%E9%9C%B2%E5%A4%A9%E6%B7%B7%E5%87%9D%E5%9C%9F%EF%BC%8C%E5%AF%92%E5%86%B7%E5%9C%B0%E5%8C%BA%E5%A4%84%E4%BA%8E%E6%B0%B4%E4%BD%8D%E5%8D%87%E9%99%8D%E8%8C%83%E5%9B%B4%E5%86%85%E7%9A%84%E6%B7%B7%E5%87%9D%E5%9C%9F%3C/span%3E" _note="" _mubu_note="">
          <outline text="优先选用：普通水泥" _mubu_text="%3Cspan%3E%E4%BC%98%E5%85%88%E9%80%89%E7%94%A8%EF%BC%9A%E6%99%AE%E9%80%9A%E6%B0%B4%E6%B3%A5%3C/span%3E" _note="" _mubu_note=""/>
