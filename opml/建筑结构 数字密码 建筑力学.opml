        <outline text="承重的毛料石柱较小边长不宜小于400mm" _mubu_text="%3Cspan%3E%E6%89%BF%E9%87%8D%E7%9A%84%E6%AF%9B%E6%96%99%E7%9F%B3%E6%9F%B1%E8%BE%83%E5%B0%8F%E8%BE%B9%E9%95%BF%E4%B8%8D%E5%AE%9C%E5%B0%8F%E4%BA%8E400mm%3C/span%3E" _note="" _mubu_note=""/>
