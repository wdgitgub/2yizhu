      <outline text="10" _mubu_text="%3Cspan%3E10%3C/span%3E" _note="" _mubu_note="">
        <outline text="约束砖砌体墙要用≥M10或Mb10（硼筑砂浆）" _mubu_text="%3Cspan%3E%E7%BA%A6%E6%9D%9F%E7%A0%96%E7%A0%8C%E4%BD%93%E5%A2%99%E8%A6%81%E7%94%A8%E2%89%A5M10%E6%88%96Mb10%EF%BC%88%E7%A1%BC%E7%AD%91%E7%A0%82%E6%B5%86%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
