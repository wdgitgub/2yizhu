      </outline>
      <outline text="40" _mubu_text="%3Cspan%3E40%3C/span%3E" _note="" _mubu_note="">
        <outline text="底部框架抗震墙砌体，房屋框架柱截面≥400mm X400mm" _mubu_text="%3Cspan%3E%E5%BA%95%E9%83%A8%E6%A1%86%E6%9E%B6%E6%8A%97%E9%9C%87%E5%A2%99%E7%A0%8C%E4%BD%93%EF%BC%8C%E6%88%BF%E5%B1%8B%E6%A1%86%E6%9E%B6%E6%9F%B1%E6%88%AA%E9%9D%A2%E2%89%A5400mm%20X400mm%3C/span%3E" _note="" _mubu_note=""/>
