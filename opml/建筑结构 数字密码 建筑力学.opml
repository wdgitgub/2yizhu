      </outline>
      <outline text="5" _mubu_text="%3Cspan%3E5%3C/span%3E" _note="" _mubu_note="">
        <outline text="除岩石地基外，基础埋深不宜小于0.5m(室外地面到基础底面)" _mubu_text="%3Cspan%3E%E9%99%A4%E5%B2%A9%E7%9F%B3%E5%9C%B0%E5%9F%BA%E5%A4%96%EF%BC%8C%E5%9F%BA%E7%A1%80%E5%9F%8B%E6%B7%B1%E4%B8%8D%E5%AE%9C%E5%B0%8F%E4%BA%8E0.5m(%E5%AE%A4%E5%A4%96%E5%9C%B0%E9%9D%A2%E5%88%B0%E5%9F%BA%E7%A1%80%E5%BA%95%E9%9D%A2)%3C/span%3E" _note="" _mubu_note=""/>
