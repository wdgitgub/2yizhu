        <outline text="受除冰盐作用环境：" _mubu_text="%3Cspan%3E%E5%8F%97%E9%99%A4%E5%86%B0%E7%9B%90%E4%BD%9C%E7%94%A8%E7%8E%AF%E5%A2%83%EF%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="海岸环境" _mubu_text="%3Cspan%3E%E6%B5%B7%E5%B2%B8%E7%8E%AF%E5%A2%83%3C/span%3E" _note="" _mubu_note=""/>
