          <outline text="桩基础的抗冲切验算" _mubu_text="%3Cspan%3E%E6%A1%A9%E5%9F%BA%E7%A1%80%E7%9A%84%E6%8A%97%E5%86%B2%E5%88%87%E9%AA%8C%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="1）柱对承台的冲切" _mubu_text="%3Cspan%3E1%EF%BC%89%E6%9F%B1%E5%AF%B9%E6%89%BF%E5%8F%B0%E7%9A%84%E5%86%B2%E5%88%87%3C/span%3E" _note="" _mubu_note="">
