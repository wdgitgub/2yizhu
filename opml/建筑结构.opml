    <outline text="拱结构" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E6%8B%B1%E7%BB%93%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
      <outline text="三铵拱在沿水平方向均匀分布的竖向荷载作用下，其合理轴线为一条抛物线" _mubu_text="%3Cspan%3E%E4%B8%89%E9%93%B5%E6%8B%B1%E5%9C%A8%E6%B2%BF%E6%B0%B4%E5%B9%B3%E6%96%B9%E5%90%91%E5%9D%87%E5%8C%80%E5%88%86%E5%B8%83%E7%9A%84%E7%AB%96%E5%90%91%E8%8D%B7%E8%BD%BD%E4%BD%9C%E7%94%A8%E4%B8%8B%EF%BC%8C%E5%85%B6%E5%90%88%E7%90%86%E8%BD%B4%E7%BA%BF%E4%B8%BA%E4%B8%80%E6%9D%A1%E6%8A%9B%E7%89%A9%E7%BA%BF%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="三铰拱在垂直于拱轴线的均匀径向荷载作用下，其合理轴线为一条圆弧线" _mubu_text="%3Cspan%3E%E4%B8%89%E9%93%B0%E6%8B%B1%E5%9C%A8%E5%9E%82%E7%9B%B4%E4%BA%8E%E6%8B%B1%E8%BD%B4%E7%BA%BF%E7%9A%84%E5%9D%87%E5%8C%80%E5%BE%84%E5%90%91%E8%8D%B7%E8%BD%BD%E4%BD%9C%E7%94%A8%E4%B8%8B%EF%BC%8C%E5%85%B6%E5%90%88%E7%90%86%E8%BD%B4%E7%BA%BF%E4%B8%BA%E4%B8%80%E6%9D%A1%E5%9C%86%E5%BC%A7%E7%BA%BF%3C/span%3E" _note="" _mubu_note="">
