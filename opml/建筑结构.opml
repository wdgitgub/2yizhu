    <outline text="延性性能" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%BB%B6%E6%80%A7%E6%80%A7%E8%83%BD%3C/span%3E" _note="" _mubu_note="">
      <outline text="延迟破坏的性能" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%BB%B6%E8%BF%9F%E7%A0%B4%E5%9D%8F%E7%9A%84%E6%80%A7%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
    </outline>
