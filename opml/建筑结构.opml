      <outline text="底部框架抗震墙砌体房屋抗震墙设置要求" _mubu_text="%3Cspan%3E%E5%BA%95%E9%83%A8%E6%A1%86%E6%9E%B6%E6%8A%97%E9%9C%87%E5%A2%99%E7%A0%8C%E4%BD%93%E6%88%BF%E5%B1%8B%E6%8A%97%E9%9C%87%E5%A2%99%E8%AE%BE%E7%BD%AE%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
