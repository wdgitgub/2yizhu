    <outline text="底部框架抗震墙砌体房屋过渡层墙体设置要求" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%BA%95%E9%83%A8%E6%A1%86%E6%9E%B6%E6%8A%97%E9%9C%87%E5%A2%99%E7%A0%8C%E4%BD%93%E6%88%BF%E5%B1%8B%E8%BF%87%E6%B8%A1%E5%B1%82%E5%A2%99%E4%BD%93%E8%AE%BE%E7%BD%AE%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
      <outline text="上部砌体墙的中心线宜与底部的框架梁、抗震墙的中心线相重合" _mubu_text="%3Cspan%3E%E4%B8%8A%E9%83%A8%E7%A0%8C%E4%BD%93%E5%A2%99%E7%9A%84%E4%B8%AD%E5%BF%83%E7%BA%BF%E5%AE%9C%E4%B8%8E%E5%BA%95%E9%83%A8%E7%9A%84%E6%A1%86%E6%9E%B6%E6%A2%81%E3%80%81%E6%8A%97%E9%9C%87%E5%A2%99%E7%9A%84%E4%B8%AD%E5%BF%83%E7%BA%BF%E7%9B%B8%E9%87%8D%E5%90%88%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="构造柱或芯柱宜与框架柱上下贯通" _mubu_text="%3Cspan%3E%E6%9E%84%E9%80%A0%E6%9F%B1%E6%88%96%E8%8A%AF%E6%9F%B1%E5%AE%9C%E4%B8%8E%E6%A1%86%E6%9E%B6%E6%9F%B1%E4%B8%8A%E4%B8%8B%E8%B4%AF%E9%80%9A%3C/span%3E" _note="" _mubu_note=""/>
