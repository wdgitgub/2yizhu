    <outline text="构造柱的设置要求" _mubu_text="%3Cspan%3E%E6%9E%84%E9%80%A0%E6%9F%B1%E7%9A%84%E8%AE%BE%E7%BD%AE%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
      <outline text="1）设置位置：" _mubu_text="%3Cspan%3E1%EF%BC%89%E8%AE%BE%E7%BD%AE%E4%BD%8D%E7%BD%AE%EF%BC%9A%3C/span%3E" _note="" _mubu_note="">
        <outline text="楼、电梯间四角，楼梯斜梯段上下端对应的墙体处外墙四角和对应转角" _mubu_text="%3Cspan%3E%E6%A5%BC%E3%80%81%E7%94%B5%E6%A2%AF%E9%97%B4%E5%9B%9B%E8%A7%92%EF%BC%8C%E6%A5%BC%E6%A2%AF%E6%96%9C%E6%A2%AF%E6%AE%B5%E4%B8%8A%E4%B8%8B%E7%AB%AF%E5%AF%B9%E5%BA%94%E7%9A%84%E5%A2%99%E4%BD%93%E5%A4%84%E5%A4%96%E5%A2%99%E5%9B%9B%E8%A7%92%E5%92%8C%E5%AF%B9%E5%BA%94%E8%BD%AC%E8%A7%92%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="错层部位横墙与外纵墙交接处大房间内外墙交接处" _mubu_text="%3Cspan%3E%E9%94%99%E5%B1%82%E9%83%A8%E4%BD%8D%E6%A8%AA%E5%A2%99%E4%B8%8E%E5%A4%96%E7%BA%B5%E5%A2%99%E4%BA%A4%E6%8E%A5%E5%A4%84%E5%A4%A7%E6%88%BF%E9%97%B4%E5%86%85%E5%A4%96%E5%A2%99%E4%BA%A4%E6%8E%A5%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="较大洞口两侧" _mubu_text="%3Cspan%3E%E8%BE%83%E5%A4%A7%E6%B4%9E%E5%8F%A3%E4%B8%A4%E4%BE%A7%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="2）构造柱截面" _mubu_text="%3Cspan%3E2%EF%BC%89%E6%9E%84%E9%80%A0%E6%9F%B1%E6%88%AA%E9%9D%A2%3C/span%3E" _note="" _mubu_note="">
        <outline text="构造柱截面最小截面180mm X240mm" _mubu_text="%3Cspan%3E%E6%9E%84%E9%80%A0%E6%9F%B1%E6%88%AA%E9%9D%A2%E6%9C%80%E5%B0%8F%E6%88%AA%E9%9D%A2180mm%20X240mm%3C/span%3E" _note="" _mubu_note=""/>
