      <outline text="1钢结构的材料" _mubu_text="%3Cspan%3E1%E9%92%A2%E7%BB%93%E6%9E%84%E7%9A%84%E6%9D%90%E6%96%99%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="2基本设计规定" _mubu_text="%3Cspan%3E2%E5%9F%BA%E6%9C%AC%E8%AE%BE%E8%AE%A1%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
