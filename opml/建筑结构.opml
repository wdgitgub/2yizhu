        <outline text="沉降缝的设置" _mubu_text="%3Cspan%3E%E6%B2%89%E9%99%8D%E7%BC%9D%E7%9A%84%E8%AE%BE%E7%BD%AE%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="1）沉降缝设置位置" _mubu_text="%3Cspan%3E1%EF%BC%89%E6%B2%89%E9%99%8D%E7%BC%9D%E8%AE%BE%E7%BD%AE%E4%BD%8D%E7%BD%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="建筑平面的转折部位" _mubu_text="%3Cspan%3E%E5%BB%BA%E7%AD%91%E5%B9%B3%E9%9D%A2%E7%9A%84%E8%BD%AC%E6%8A%98%E9%83%A8%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="高度差异或荷载差异处" _mubu_text="%3Cspan%3E%E9%AB%98%E5%BA%A6%E5%B7%AE%E5%BC%82%E6%88%96%E8%8D%B7%E8%BD%BD%E5%B7%AE%E5%BC%82%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="长高比过大的砌体承重结构或钢筋混凝土框架结构" _mubu_text="%3Cspan%3E%E9%95%BF%E9%AB%98%E6%AF%94%E8%BF%87%E5%A4%A7%E7%9A%84%E7%A0%8C%E4%BD%93%E6%89%BF%E9%87%8D%E7%BB%93%E6%9E%84%E6%88%96%E9%92%A2%E7%AD%8B%E6%B7%B7%E5%87%9D%E5%9C%9F%E6%A1%86%E6%9E%B6%E7%BB%93%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="的适当部位" _mubu_text="%3Cspan%3E%E7%9A%84%E9%80%82%E5%BD%93%E9%83%A8%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="地基土的压缩性有显著差异处" _mubu_text="%3Cspan%3E%E5%9C%B0%E5%9F%BA%E5%9C%9F%E7%9A%84%E5%8E%8B%E7%BC%A9%E6%80%A7%E6%9C%89%E6%98%BE%E8%91%97%E5%B7%AE%E5%BC%82%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="建筑结构或基础类型不同处" _mubu_text="%3Cspan%3E%E5%BB%BA%E7%AD%91%E7%BB%93%E6%9E%84%E6%88%96%E5%9F%BA%E7%A1%80%E7%B1%BB%E5%9E%8B%E4%B8%8D%E5%90%8C%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="分期建造房屋的交界处" _mubu_text="%3Cspan%3E%E5%88%86%E6%9C%9F%E5%BB%BA%E9%80%A0%E6%88%BF%E5%B1%8B%E7%9A%84%E4%BA%A4%E7%95%8C%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
