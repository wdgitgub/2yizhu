      <outline text="1）设置条件：" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E1%EF%BC%89%E8%AE%BE%E7%BD%AE%E6%9D%A1%E4%BB%B6%EF%BC%9A%3C/span%3E" _note="" _mubu_note="">
        <outline text="多层砌体防震缝的设置在房屋立面高差&gt;6m的地方" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%A4%9A%E5%B1%82%E7%A0%8C%E4%BD%93%E9%98%B2%E9%9C%87%E7%BC%9D%E7%9A%84%E8%AE%BE%E7%BD%AE%E5%9C%A8%E6%88%BF%E5%B1%8B%E7%AB%8B%E9%9D%A2%E9%AB%98%E5%B7%AE&amp;gt;6m%E7%9A%84%E5%9C%B0%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="多层砌体防震缝的设置在，房屋有错层，且楼板高差&gt;层高×1/4" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%A4%9A%E5%B1%82%E7%A0%8C%E4%BD%93%E9%98%B2%E9%9C%87%E7%BC%9D%E7%9A%84%E8%AE%BE%E7%BD%AE%E5%9C%A8%EF%BC%8C%E6%88%BF%E5%B1%8B%E6%9C%89%E9%94%99%E5%B1%82%EF%BC%8C%E4%B8%94%E6%A5%BC%E6%9D%BF%E9%AB%98%E5%B7%AE&amp;gt;%E5%B1%82%E9%AB%98%C3%971/4%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="多层砌体防震缝的设置在各部分结构刚度、质量截然不同的地方" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E5%A4%9A%E5%B1%82%E7%A0%8C%E4%BD%93%E9%98%B2%E9%9C%87%E7%BC%9D%E7%9A%84%E8%AE%BE%E7%BD%AE%E5%9C%A8%E5%90%84%E9%83%A8%E5%88%86%E7%BB%93%E6%9E%84%E5%88%9A%E5%BA%A6%E3%80%81%E8%B4%A8%E9%87%8F%E6%88%AA%E7%84%B6%E4%B8%8D%E5%90%8C%E7%9A%84%E5%9C%B0%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="2）防震缝宽度：" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E2%EF%BC%89%E9%98%B2%E9%9C%87%E7%BC%9D%E5%AE%BD%E5%BA%A6%EF%BC%9A%3C/span%3E" _note="" _mubu_note="">
        <outline text="70mnm-100mm" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E70mnm-100mm%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
    </outline>
