      <outline text="转换层的位置：7度≤5层" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E8%BD%AC%E6%8D%A2%E5%B1%82%E7%9A%84%E4%BD%8D%E7%BD%AE%EF%BC%9A7%E5%BA%A6%E2%89%A45%E5%B1%82%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="6度≤可适当提高" _mubu_text="%3Cspan%3E6%E5%BA%A6%E2%89%A4%E5%8F%AF%E9%80%82%E5%BD%93%E6%8F%90%E9%AB%98%3C/span%3E" _note="" _mubu_note=""/>
